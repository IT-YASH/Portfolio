<div class="author">
    <h1 class="author_heading">Search Results</h1>
  </div>
  
  <section class="articles">
    <ul class="article-ul">
      <% data.forEach(post => { %>
      <li>
        <a href="/project-search.ejs/<%= post._id %>">
          <span><%= post.Project_title %></span>
          <span class="article-list_date"
            ><%= post.createdAt.toDateString() %></span
          >
        </a>
      </li>
      <% }) %>
    </ul>
  </section>
  